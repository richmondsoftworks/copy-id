{
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true
  },
  "editor.formatOnSave": true,
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/*.exe": true,
    "**/bin": true,
    "**/build": true,
    "**/CVS": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/obj": true,
    "**/storybook-static": true
  },
  "importSorter.importStringConfiguration.quoteMark": "double",
  "importSorter.generalConfiguration.sortOnBeforeSave": true,
  "importSorter.sortConfiguration.joinImportPaths": true,
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.count": 999,
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.type": "maxLineLength"
  // "prettier.tslintIntegration": true,
}
